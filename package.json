{
  "name": "bryce-finds-an-apartment",
  "version": "0.0.1",
  "description": "a small google maps app to help me figure out where to live",
  "main": "index.js",
  "scripts": {
    "lint:prod": "eslint ./",
    "lint:dev": "eslint ./ --rule 'no-console: 0'|| true",
    "test": "istanbul cover _mocha; npm run lint:dev",
    "clean": "rm -fdr ./build ./dist",
    "build": "npm run lint:prod && mkdir -p build && ./package",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brycekbargar/bryce-finds-an-apartment.git"
  },
  "author": "bryce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brycekbargar/bryce-finds-an-apartment/issues"
  },
  "homepage": "https://github.com/brycekbargar/bryce-finds-an-apartment#readme",
  "devDependencies": {
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "istanbul": "^0.4.3",
    "jsdomify": "^2.1.0",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.9",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "google-maps": "^3.2.1",
    "local-storage": "^1.4.2",
    "mithril": "^0.2.4"
  }
}
